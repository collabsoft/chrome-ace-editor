{
  "name": "ACE Editor",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "app/icons/38x38.png",
      "38": "app/icons/38x38.png"
    },
    "default_title": "That's the tool tip",
    "default_popup": "app/index.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["app/main.js"]
  }],
  "permissions": [
    "activeTab"
  ]
}